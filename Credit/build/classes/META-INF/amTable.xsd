<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" targetNamespace="http://itq.edu/soa/amTable">
	<xs:element name="request">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="quantiti" type="xs:float"></xs:element>
				<xs:element name="interest" type="xs:float"></xs:element>
				<xs:element name="time" type="xs:int"></xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="response">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="amortization" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="periodo" type="xs:int"></xs:element>
							<xs:element name="capital" type="xs:float"></xs:element>
							<xs:element name="pagoCapital" type="xs:float"></xs:element>
							<xs:element name="pagoInteres" type="xs:float"></xs:element>
							<xs:element name="montoMensual" type="xs:float"></xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
